def square_to_board_index(square):
    square_file = 'abcdefgh'.index(square[0])
    square_rank = '87654321'.index(square[1])
    return square_file, square_rank

def square_to_board_lookup(square, board):
    index_file, index_rank = square_to_board_index(square)
    return board[index_rank][index_file]

